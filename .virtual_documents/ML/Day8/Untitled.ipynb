import json

def load_data(filename):
    with open(filename, "r") as f:
        data = json.load(f)
    return data




data = load_data("store_data.json")
print(data)
print(type(data))  


# data cleaning
def view_data(data):
    for user in data:
        print(user)


view_data(data)


# clean data
def clean_data(data):
    text_to_num = {"one":1, "two":2, "three":3, "four":4, "five":5}

    for users in data:
        raw_rating = users["rating"].strip().trim()
        print(raw_rating)
        if(raw_rating in text_to_num):
            raw_rating = text_to_num[raw_rating]
        users["rating"] = raw_rating




clean_data(data)


view_data(data)



